{
  "name": "@moisespsena/vuetify-editable-image-field",
  "repository": "https://github.com/moisespsena-js/vuetify-editable-image-field",
  "author": "Mois√©s P. Sena",
  "version": "0.0.1",
  "private": false,
  "description": "Vuetify image field component with editor",
  "type": "module",
  "keywords": [
    "vue",
    "vuetify",
    "component",
    "input",
    "field",
    "editor",
    "picture",
    "image",
    "image-field",
    "image-input"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build ",
    "build:lib": "cross-env NODE_ENV=lib babel src --out-dir lib --source-maps --extensions \".ts\",\".tsx\",\".snap\" --copy-files --no-copy-ignored --out-file-extension .mjs",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "tui-image-editor": "^3.15.3",
    "vue": "^3.5.13",
    "deviceorientation-js": "^1.0.0",
    "rotate-canvas": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@mdi/font": "7.4.47",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "typescript": "^5.4.3",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.6",
    "vite-plugin-vuetify": "^2.0.3",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7",
    "vuetify": "^3.7.14"
  }
}
